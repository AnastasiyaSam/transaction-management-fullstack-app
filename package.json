{
  "name": "account-management-fullstack-level-1",
  "version": "1.0.0",
  "scripts": {
    "start:backend": "cd backend && node index.js",
    "start:frontend": "cd frontend && node .output/server/index.mjs",
    "start": "npm-run-all --parallel start:backend start:frontend",
    "test": "cypress run",
    "build:backend": "cd backend && npm i",
    "build:frontend": "cd frontend && npm i && npm run build",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "cypress": "^12.17.1",
    "npm-run-all": "^4.1.5"
  }
}
